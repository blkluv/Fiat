{
  "name": "gridfire",
  "version": "0.1",
  "description": "A music sales platform.",
  "repository": "https://github.com/csleary/gridfire",
  "main": "index.js",
  "type": "module",
  "exports": {
    "./*": "./*"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon --ignore client/ -r dotenv/config index.js dotenv_config_path=.env",
    "client": "yarn --cwd ./client/ start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\" \"yarn --cwd ./worker/ run dev\" \"npm run ganache\"",
    "worker": "yarn --cwd ./worker/ run dev",
    "ganache": "ganache-cli --db ~/.ganache-db -i 5555 -m \"wreck arena bar club flat identify eternal exhaust poet deputy danger canyon\""
  },
  "author": "Christopher Leary",
  "license": "ISC",
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "amqplib": "^0.8.0",
    "archiver": "^5.3.0",
    "axios": "^0.26.1",
    "busboy": "^1.0.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^2.0.0",
    "dotenv": "^16.0.0",
    "ethers": "^5.5.2",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "ipfs-http-client": "^56.0.1",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.34",
    "mongoose": "^6.1.2",
    "nodemailer": "^6.4.18",
    "passport": "^0.5.2",
    "passport-custom": "^1.1.1",
    "passport-local": "^1.0.0",
    "sharp": "^0.30.3",
    "slugify": "^1.4.5"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.13",
    "@types/mime-types": "^2.1.1",
    "@types/passport": "^1.0.7",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.7"
  }
}
